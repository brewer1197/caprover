    "caproverOneClickApp": {
      "displayName": "Universal PHP Site",
      "description": "Apache + PHP. Сам заливай WordPress или HTML.",
      "isOfficial": false,
      "documentation": "https://www.php.net/manual/en/"
    },
    "variables": []
  }
]
