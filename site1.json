[
  {
    "captainVersion": "4",
    "services": [
      {
        "image": "php:8.2-apache",
        "containerHttpPort": 80,
        "volumes": [
          {
            "hostPath": "/captain/data/%%APPNAME%%",
            "containerPath": "/var/www/html"
          }
        ]
      }
